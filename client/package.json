{
  "name": "@geoserver-viewer/client",
  "private": true,
  "version": "0.0.0",
  "description": "Client application for GeoServer Viewer",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "type-check": "tsc -b --noEmit",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "format": "prettier --write \"{src,tests}/**/*.{ts,tsx,json,css}\"",
    "format-check": "prettier --check \"{src,tests}/**/*.{ts,tsx,json,css}\"",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.17",
    "@uidotdev/usehooks": "^2.4.1",
    "clsx": "^2.1.1",
    "fast-xml-parser": "^5.3.2",
    "maplibre-gl": "^5.14.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-map-gl": "^8.1.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.17"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "4.0.16",
    "@vitest/ui": "4.0.16",
    "babel-plugin-react-compiler": "^1.0.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "jsdom": "^27.3.0",
    "msw": "^2.12.4",
    "prettier": "^3.7.4",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vitest": "^4.0.16"
  }
}
